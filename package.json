{
  "name": "bootstrap-audio-player",
  "version": "1.0.0",
  "description": "JQuery Plugin for Playing Audio Files",
  "repository": {
    "type": "git",
    "url": "git@github.com:DanielJAch/bootstrap-audio-player.git"
  },
  "scripts": {
    "watch": "webpack --watch",
    "build": "webpack --config ./build/webpack.dev.config.js",
    "build-test": "webpack --config ./build/webpack.test.config.js",
    "build-staging": "webpack --config ./build/webpack.staging.config.js",
    "build-prod": "webpack --config ./build/webpack.prod.config.js",
    "clean": "rmdir /s /q node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "author": {
    "name": "Daniel Ach",
    "email": "danieljach@gmail.com"
  },
  "keywords": [
    "bootstrap-plugin",
    "jquery-plugin",
    "audio",
    "player",
    "bootstrap-audio-player"
  ],
  "license": "MIT",
  "main": "dist/js/jquery.audioPlayer.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "jquery": ">=3.4.1",
    "bootstrap": ">=4.3.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "3.0.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1"
  }
}
